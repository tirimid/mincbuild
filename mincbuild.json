{
    "tc": {
        "cc": "/usr/bin/gcc",
        "ld": "/usr/bin/gcc",
        "cflags": "-std=c99 -pedantic",
        "ldflags": ""
    },
    "tc-info": {
        "cc-conly-flag": "-c",
		"cc-inc-flag": "-I",
        "cc-cobj-flag": "-o",
        "ld-lib-flag": "-l",
        "ld-lbin-flag": "-o"
    },
    "proj": {
        "src-dir": "src",
        "inc-dir": "include",
        "lib-dir": "lib",
        "produce-output": true,
        "output": "buildc-test",
        "src-exts": ["c"],
		"hdr-exts": ["h"]
    },
    "deps": {
        "incs": [],
        "libs": ["json-c", "pthread"]
    }
}
