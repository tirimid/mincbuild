{
	"tc": {
		"cc": "/usr/bin/gcc",
		"ld": "/usr/bin/gcc",
		"cflags": "-std=c99 -pedantic -D_POSIX_C_SOURCE=200809",
		"ldflags": ""
	},
	"tc-info": {
		"cc-inc-fmt": "-I%i",
		"cc-cmd-fmt": "%c %f -o %o -c %s %i",
		"ld-lib-fmt": "-l%l",
		"ld-obj-fmt": "%o",
		"ld-cmd-fmt": "%c %f -o %b %o %l",
		"cc-success-rc": 0,
		"ld-success-rc": 0
	},
	"proj": {
		"src-dir": "src",
		"inc-dir": "include",
		"lib-dir": "lib",
		"produce-output": true,
		"output": "mincbuild",
		"src-exts": ["c"],
		"hdr-exts": ["h"]
	},
	"deps": {
		"incs": [],
		"libs": ["json-c", "pthread"]
	}
}
